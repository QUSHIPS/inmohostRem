body {
	background: #333333;
}
html, body {
	font-family:Verdana, Arial, Helvetica, sans-serif;
	margin:0px;
	padding:0px;
	height:100%;
}
body, table, td, samp, div {
	font-family:Verdana, Arial, Helvetica, sans-serif;
	font-size: 12px;
	color:#FFFFFF;
}
a, a:link, a:visited, a:active {
	color: #FFFFFF;
	text-decoration:none;
}
a:hover{
	color:#FF6600;
	text-decoration: underline;
}
h1, H1 {
  font-family:Verdana, Arial, Helvetica, sans-serif;
  font-size: 11px;
  display: block;
  color: #FFFFFF;
  margin:0px;
  font-weight:bold;
}
a{ cursor: pointer;  }
h2, H2 {
  font-family:Verdana, Arial, Helvetica, sans-serif;
  font-size: 12px;
  display: block;
  color:#EEEEEE;
  margin:0px;
}
form{
	margin:0px;
	padding:0px;
}
img {
	border : 0 none;
}
HR{
	height:1px;
}
.botonForm {
	background-color:#FF9900;
	font-size: 10px;
	color: #333333;
	border: 1px solid;
	border-top-color: #999999;
	border-right-color: #333333;
	border-bottom-color: #333333;
	border-left-color: #999999;
}
.inputForm {
	background-color:#EEEEEE;
	font-size: 10px;
	color: #333333;
	border: 1px solid;
	border-top-color: #333333;
	border-right-color: #999999;
	border-bottom-color: #999999;
	border-left-color: #333333;
}
.tableOscura{
	background:#444444;
}
.tableClara{
	background:#EEEEEE;
}
.destacado{
	background:#FF6600;
	color:#FFFFFF;
	font-weight:bold;
}
.menuItem, .menuItem:link, menuItem:visited, menuItem:active {
  font-size: 10px;
  display: block;
  color:#FFFFFF;
  text-decoration:none;
}
.menuItem:hover{
  font-size: 10px;
  display: block;
  color:#FFFFFF;
	background-color: #FF9900;
	text-decoration:none;
}
