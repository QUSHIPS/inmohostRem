body, table, td, samp, div {
	font-family:Verdana, Arial, Helvetica, sans-serif;
	font-size: 10px;
	color:#000000;
}
html, body{
	font-family:Verdana, Arial, Helvetica, sans-serif;
	margin:0px;
	padding:0px;
	width:100%;
	height:100%;
	background: #FFFFFF;
}
a:link, a:visited, a:active {
	color: #333333;
	text-decoration:none;
}
a:hover{
	color: #333333;
	text-decoration: none;
}
a{ cursor: pointer;  }
h1, h1:link, h1:hover, h1:visited, h1:active{
  font-family:Verdana, Arial, Helvetica, sans-serif;
  font-size: 12px;
  font-weight:bold;
  display: block;
  color: #3366CC;
  margin:0px;
}
h2{
  font-family:Verdana, Arial, Helvetica, sans-serif;
  font-size: 10px;
  display: block;
  color:#3366CC;
  margin:0px;
}
h3{
  font-family:Verdana, Arial, Helvetica, sans-serif;
  font-size: 10px;
  display: block;
  color: #333333;
  margin:0px;
}
form{
	margin:0px;
	padding:0px;
}
img {
	border : 0 none;
}
HR{
	height:1px;
	background:#3366CC;
	border-color:#3366CC;
}
ul{
	margin:3px;
	padding:3px;
	color:#3366CC;
	list-style-type: disc;
	list-style-position: inside;
}
ul{
	color:#333333;
}

/*			CLASSES				*/
.botonForm {
	background-color:#3366CC;
	font-size: 10px;
	color: #FFFFFF;
	border: 1px solid;
	border-top-color: #999999;
	border-right-color: #333333;
	border-bottom-color: #333333;
	border-left-color: #999999;
}
.inputForm {
	font-family:Verdana, Arial, Helvetica, sans-serif;
	background-color:#FFFFFF;
	font-size: 10px;
	color: #666666;
	border: 1px solid;
	border-top-color: #333333;
	border-right-color: #999999;
	border-bottom-color: #999999;
	border-left-color: #333333;
}
.inputFecha {
	font-size: 10px;
	font-weight:bold;
	color: #222222;
	border: 0px;
}
.tableOscura{
	background:#3366CC;	
}
.tableClara{
	background:#FFFFFF;
}
.temporal {
	background:#FFFFEA;
	cursor:pointer;
}
.destacado {
	background:#AADEFD;
}
