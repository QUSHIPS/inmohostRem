.overlay_inmohost {
	background-color: #FFFFFF;
/*	filter:alpha(opacity=60); */
	-moz-opacity: 0.6;
	opacity: 0.6;
}

.inmohost_nw {
	background: transparent url(images/window/esquinaIzqSup.png) no-repeat 0 0;
  width:9px;
  height:25px;
}

.inmohost_n {
  background: transparent url(images/window/superior.png) repeat-x 0 0;
  height:25px;
}

.inmohost_ne {
  background: transparent url(images/window/esquinaDerSup.png) no-repeat 0 0;
  width:15px;	  
  height:25px;
}

.inmohost_w {
  background: transparent url(images/window/izquierdo.png) repeat-y top left;
  width:5px;
}

.inmohost_e {
  background: transparent url(images/window/derecha.png) repeat-y top right;
  width:12px;
}

.inmohost_sw {
  background: transparent url(images/window/esquinaIzqInf.png) no-repeat 0 0;
  width:9px;
  height:15px;
}

.inmohost_s  {
  background: transparent url(images/window/inferior.png) repeat-x 0 0;			
  height:15px;
}

.inmohost_se {
  background: transparent url(images/window/esquinaDerInf.png) no-repeat 0 0;
  width:15px;
  height:15px;
}
.inmohost_sizer {
  background: transparent url(images/window/esquinaDerInf.png) no-repeat 0 0;
  width:15px;
  height:15px;
}

.inmohost_sizer {
	cursor:se-resize;	
}

.inmohost_close {
	width: 23px;
	height: 23px;
	background: transparent url(images/window/button-close-focus.gif) no-repeat 0 0;
	position:absolute;
	top:0px;
	right:11px;
	cursor:pointer;
	z-index:1000;
}

.inmohost_minimize {
	width: 23px;
	height: 23px;
	background: transparent url(images/window/button-min-focus.gif) no-repeat 0 0;
	position:absolute;
	top:0px;
	right:55px;
	cursor:pointer;
	z-index:1000;
}

.inmohost_maximize {
	width: 23px;
	height: 23px;
	background: transparent url(images/window/button-max-focus.gif) no-repeat 0 0;
	position:absolute;
	top:0px;
	right:33px;
	cursor:pointer;
	z-index:1000;
}

.inmohost_title {
	float:left;
	height:14px;
	font-size:14px;
	font-weight:bold;
	text-align:left;
	margin-top:0px;
	width:100%;
	color:#333333;
}

.inmohost_content {
	overflow:auto;
	color: #000000;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 12px;
	background:#FFFFFF;
}
.dialog_content {
	overflow:auto;
	color: #333333;
	font-family: Tahoma, Arial, sans-serif;
	font-size: 10px;
	background-color:#FFFFFF;
}

.top_draggable, .bottom_draggable {
  cursor:move;
}
.wired_frame {
	display: block;
	position: absolute;
	border: 1px #000 dashed;
}
.status_bar {
  font-size:10px;
  font-family:Verdana, Arial, Helvetica, sans-serif;
  margin-bottom:13px;
}
.status_bar input{
  font-size:10px;
  font-family:Verdana, Arial, Helvetica, sans-serif;
}
/* DO NOT CHANGE THESE VALUES*/
.dialog {
	display: block;
	position: absolute;
}

.dialog table.table_window  { 
	border-collapse: collapse; 
	border-spacing: 0; 
	width: 100%;
	margin: 0px;
	padding:0px;
}

.dialog table.table_window td, .dialog table.table_window th { 
  padding: 0; 
}

.dialog .title_window {
  -moz-user-select:none;
}

